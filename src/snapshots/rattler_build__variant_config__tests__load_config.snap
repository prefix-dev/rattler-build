    max_pin: x.x.x
    min_pin: ~
  boost-cpp:
    max_pin: x.x.x
    min_pin: ~
  flann:
    max_pin: x.x.x
    min_pin: ~
  graphviz:
    max_pin: x
    min_pin: ~
  libsvm:
    max_pin: x
    min_pin: ~
  netcdf-cxx4:
    max_pin: x.x
    min_pin: ~
  occt:
    max_pin: x.x
    min_pin: ~
  poppler:
    max_pin: x.x
    min_pin: ~
  r-base:
    max_pin: x.x
    min_pin: x.x
  vlfeat:
    max_pin: x.x.x
    min_pin: ~
zip_keys:
  - - c_compiler_version
    - cxx_compiler_version
    - fortran_compiler_version
    - docker_image
  - - cudnn
    - cuda_compiler_version
  - - python
    - numpy
    - python_impl
  - - arrow_cpp
