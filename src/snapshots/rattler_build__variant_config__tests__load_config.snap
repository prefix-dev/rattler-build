  - openblas
  - mkl
  - blis
build_platform:
  - linux-64
libblas:
  - 3.9 *netlib
libcblas:
  - 3.9 *netlib
liblapack:
  - 3.9 *netlib
liblapacke:
  - 3.9 *netlib
target_platform:
  - linux-64
