build:
  number: 0
  string: blabla
  script:
    env:
      TEST: MYENV_VAR
    secrets:
    - SECRET_VARIABLE
    content:
    - test
  noarch: generic
  python:
    entry_points:
    - test = test:main
    skip_pyc_compilation:
    - test/bla/**/*.py
    use_python_app_entrypoint: true
  dynamic_linking:
    rpaths:
    - lib/
    binary_relocation:
    - lib/*
    missing_dso_allowlist:
    - /all/lib/**/*
    rpath_allowlist:
    - bla/*
    overdepending_behavior: error
  always_copy_files:
  - file1
  - file2/**/*
  always_include_files:
  - file1
  - file2/**/*
  merge_build_and_host_envs: true
  variant:
    use_keys:
    - key1
    - key2
    ignore_keys:
    - python
    - numpy
    down_prioritize_variant: -5
  prefix_detection:
    force_file_type:
      text:
      - bla/*.txt
      binary:
      - bla/*.so
    ignore: true
    ignore_binary_files: true
