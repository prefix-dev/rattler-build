---
source: crates/rattler_build_recipe/src/stage0/parser/snapshot_tests.rs
expression: "serde_yaml::to_string(&recipe).unwrap()"
---
package:
  name: ${{ name | lower }}
  version: ${{ version }}
build:
  number: 0
  string: null
  script: []
  noarch: null
  python:
    entry_points: []
    skip_pyc_compilation: []
    use_python_app_entrypoint: false
    version_independent: false
    site_packages_path: null
  skip: []
  always_copy_files: []
  always_include_files: []
  merge_build_and_host_envs: false
  files: []
  dynamic_linking:
    rpaths: []
    binary_relocation: true
    missing_dso_allowlist: []
    rpath_allowlist: []
    overdepending_behavior: null
    overlinking_behavior: null
  variant:
    use_keys: []
    ignore_keys: []
    down_prioritize_variant: null
  prefix_detection:
    force_file_type:
      text: []
      binary: []
    ignore: false
    ignore_binary_files: false
  post_process: []
requirements:
  build:
  - ${{ compiler("c") }}
  - ${{ compiler("cxx") }}
  - if: unix
    then: make
    else: []
  - if: win
    then: cmake
    else: []
  host:
  - python ${{ python }}
  - setuptools >=45
  - wheel
  - if: build_type == "cuda"
    then: cuda-toolkit ${{ cuda_version }}
    else: []
  run:
  - python ${{ python }}
  - numpy >=1.19,<2.0a0
  - ${{ pin_compatible("numpy") }}
  - if: linux
    then: libgcc-ng >=9.3.0
    else: []
  - if: osx
    then: libcxx >=10.0.0
    else: []
  - if: build_type == "cuda"
    then: cuda-toolkit ${{ cuda_version }}
    else: []
  run_constraints:
  - scipy >=1.5,<2.0a0
  - if: extra_deps
    then: pandas >=1.0
    else: []
  run_exports:
    strong:
    - ${{ pin_subpackage(name, max_pin="x.x") }}
    weak:
    - if: has_cpp_lib
      then: libstdcxx-ng >=9.3.0
      else: []
  ignore_run_exports:
    by_name:
    - qt
    - tk
    from_package:
    - python
about:
  homepage: https://github.com/${{ org }}/${{ name }}
  license: ${{ license }}
  license_file:
  - LICENSE.txt
  license_family: null
  summary: ${{ summary }}
  description: |
    ${{ description }}

    This is a multi-line description that can contain
    templates and other formatting.
  documentation: https://${{ name }}.readthedocs.io
  repository: https://github.com/${{ org }}/${{ name }}
extra:
  recipe-maintainers:
  - ${{ maintainer1 }}
  - ${{ maintainer2 }}
