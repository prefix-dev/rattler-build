      overlinking_behavior: null
    variant:
      use_keys: []
      ignore_keys: []
      down_prioritize_variant: null
    prefix_detection:
      force_file_type:
        text: []
        binary: []
      ignore: false
      ignore_binary_files: false
    post_process: []
  about:
    homepage: https://example.com/${{ name }}
    license: MIT
    license_family: null
    summary: Package ${{ name }} version ${{ version }}
    description: null
    documentation: null
    repository: null
- package:
