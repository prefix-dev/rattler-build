      then: make
      else: []
    - if: win
      then: nmake
      else: []
    host:
    - python ${{ python_version }}
  build:
    script:
    - if: unix
      then: make -j${{ CPU_COUNT }}
      else: []
    - if: win
      then: nmake
      else: []
- package:
    name: ${{ name }}
    version: ${{ version }}
  inherit: ${{ staging_name }}
  requirements:
    run:
    - python ${{ python_version }}.*
    - ${{ pin_subpackage(name ~ "-lib", max_pin="x.x") }}
